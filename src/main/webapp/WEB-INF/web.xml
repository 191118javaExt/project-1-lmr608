<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<display-name>project-1-lmr608</display-name>
  	<welcome-file-list>
    	<welcome-file>index.html</welcome-file>
    	<welcome-file>index.htm</welcome-file>
    	<welcome-file>index.jsp</welcome-file>
    	<welcome-file>default.html</welcome-file>
    	<welcome-file>default.htm</welcome-file>
    	<welcome-file>default.jsp</welcome-file>
 	</welcome-file-list>
	<filter>
		<filter-name>CORS</filter-name>
		<filter-class>com.thetransactioncompany.cors.CORSFilter</filter-class>
		<init-param>
			<param-name>cors.allowed.origins</param-name>
			<param-value>https://localhost:4200</param-value>
		</init-param>
	</filter>
	<filter-mapping>
		<filter-name>CORS</filter-name>
		<url-pattern>/*</url-pattern>
	</filter-mapping>
	<servlet>
		<servlet-name>LoginServlet</servlet-name>
		<servlet-class>com.revature.servlets.LoginServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>LogoutServlet</servlet-name>
		<servlet-class>com.revature.servlets.LogoutServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LogoutServlet</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet>
	<servlet-name>ManagerHomepageServlet</servlet-name>
		<servlet-class>com.revature.servlets.ManagerHomepageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ManagerHomepageServlet</servlet-name>
		<url-pattern>/manager</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>EmployeeHomepageServlet</servlet-name>
		<servlet-class>com.revature.servlets.EmployeeHomepageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>EmployeeHomepageServlet</servlet-name>
		<url-pattern>/employee</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SubmitReimbursementServlet</servlet-name>
		<servlet-class>com.revature.servlets.SubmitReimbursementServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SubmitReimbursementServlet</servlet-name>
		<url-pattern>/employee/submit</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>PendingReimbursementsServlet</servlet-name>
		<servlet-class>com.revature.servlets.PendingReimbursementsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>PendingReimbursementsServlet</servlet-name>
		<url-pattern>/employee/pending</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ResolvedReimbursementsServlet</servlet-name>
		<servlet-class>com.revature.servlets.ResolvedReimbursementsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ResolvedReimbursementsServlet</servlet-name>
		<url-pattern>/employee/resolved</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ViewAllEmployeesServlet</servlet-name>
		<servlet-class>com.revature.servlets.EmployeeHomepageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ViewAllEmployeesServlet</servlet-name>
		<url-pattern>/manager/employees</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ViewAllReimbursementsServlet</servlet-name>
		<servlet-class>com.revature.servlets.ViewAllReimbursementsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ViewAllReimbursementsServlet</servlet-name>
		<url-pattern>/manager/reimbursements</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>ApproveDenyReimbursementsServlet</servlet-name>
		<servlet-class>com.revature.servlets.ApproveDenyReimbursementsServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ApproveDenyReimbursementsServlet</servlet-name>
		<url-pattern>/manager/approvedeny</url-pattern>
	</servlet-mapping>
</web-app>